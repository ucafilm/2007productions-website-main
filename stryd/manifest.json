{
  "name": "Stryd Stories",
  "short_name": "StrydStories",
  "description": "Transform your Stryd run data into Instagram Stories with custom overlays and themes",
  "start_url": "/stryd/",
  "scope": "/stryd/",
  "display": "standalone",
  "theme_color": "#ff6b35",
  "background_color": "#000000",
  "orientation": "portrait",
  "categories": ["fitness", "social", "productivity"],
  "lang": "en",
  "dir": "ltr",
  "icons": [
    {
      "src": "icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "New Story",
      "short_name": "New",
      "description": "Create a new Stryd Instagram Story",
      "url": "/stryd/?action=new",
      "icons": [{ "src": "icons/shortcut-new.png", "sizes": "96x96" }]
    },
    {
      "name": "Gallery",
      "short_name": "Gallery",
      "description": "View saved stories",
      "url": "/stryd/?action=gallery",
      "icons": [{ "src": "icons/shortcut-gallery.png", "sizes": "96x96" }]
    }
  ],
  "share_target": {
    "action": "/stryd/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "image",
          "accept": ["image/png", "image/jpeg", "image/jpg"]
        }
      ]
    }
  },
  "related_applications": [],
  "prefer_related_applications": false
}