{
  "name": "Stryd Stories",
  "short_name": "StrydStories",
  "description": "Transform your Stryd run data into Instagram Stories with custom overlays and themes",
  "start_url": "/stryd/",
  "scope": "/stryd/",
  "display": "standalone",
  "theme_color": "#ff6b35",
  "background_color": "#000000",
  "orientation": "portrait",
  "categories": ["fitness", "social", "productivity"],
  "lang": "en",
  "dir": "ltr",
  "icons": [
    {
      "src": "icons/icon-512x512.svg",
      "sizes": "72x72 96x96 128x128 144x144 152x152 192x192 384x384 512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "icons/icon-512x512.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "New Story",
      "short_name": "New",
      "description": "Create a new Stryd Instagram Story",
      "url": "/stryd/?action=new"
    }
  ],
  "share_target": {
    "action": "/stryd/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "image",
          "accept": ["image/png", "image/jpeg", "image/jpg"]
        }
      ]
    }
  },
  "related_applications": [],
  "prefer_related_applications": false
}